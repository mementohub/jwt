{
    "name": "imemento/jwt",
    "description": "Custom JWT logic",
    "homepage": "https://github.com/mementohub/jwt",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\JWT\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
