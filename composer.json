{
    "name": "imemento/jwt",
    "description": "Custom JWT logic",
    "homepage": "https://gitlab.com/imemento/composer/packages/jwt",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "firebase/php-jwt": "^5.0.0",
        "imemento/exceptions": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\JWT\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
        {"packagist.org": false}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
